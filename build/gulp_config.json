{
    "js": {
        "src": [
            "./src/js/*.js"
        ],
        "concat": {
            "main": "index.js"
        },
        "concat_min": {
            "main": "index.min.js"
        },
        "dest": "./dist/js/"
    },
    "css": {
        "src": [
            "./src/css/*.css"
        ],
        "concat": {
            "main": "style.css"
        },
        "concat_min": {
            "main": "style.min.css"
        },
        "dest": "./dist/css/"
    },
    "html": {
        "index": "./src/html/index.html",
        "src": [
            "./**/*.html"
        ],
        "dest": ""
    },
    "rev": {
        "src": "./dist/rev/*.json",
        "dest": "./dist/rev",
        "destH": "./src/html/"
    }
}